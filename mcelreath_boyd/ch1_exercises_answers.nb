Cell["\<\
See also \t for a good piece of cobweb plot. However, for the sake of \
documentation I have made my own plot here (let me know if it breaks down):\
\>", "Text",
 CellChangeTimes->{{3.795972973297288*^9, 3.795973010830686*^9}, 
   3.796412690362913*^9, 
   3.796412936450445*^9},ExpressionUUID->"2fb3633b-3c3a-4494-bbc6-\
4fc7a2abce08"],

Cell[BoxData[{
 RowBox[{"ClearAll", "[", "CobwebPlot", "]"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"Options", "[", "CobwebPlot", "]"}], "=", 
   RowBox[{"Join", "[", 
    RowBox[{
     RowBox[{"{", 
      RowBox[{"CobStyle", "\[Rule]", "Automatic"}], "}"}], ",", 
     RowBox[{"Options", "[", "Graphics", "]"}]}], "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"CobwebPlot", "[", 
   RowBox[{"f_", ",", 
    RowBox[{"start_", "?", "NumericQ"}], ",", "n_", ",", 
    RowBox[{"xrange", ":", 
     RowBox[{"{", 
      RowBox[{"xmin_", ",", "xmax_"}], "}"}]}], ",", 
    RowBox[{"opts", ":", 
     RowBox[{"OptionsPattern", "[", "]"}]}]}], "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"cob", ",", "x", ",", "g1", ",", "coor"}], "}"}], ",", 
    RowBox[{
     RowBox[{"cob", "=", 
      RowBox[{"NestList", "[", 
       RowBox[{"f", ",", 
        RowBox[{"N", "[", "start", "]"}], ",", "n"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"coor", "=", 
      RowBox[{"Partition", "[", 
       RowBox[{
        RowBox[{"Riffle", "[", 
         RowBox[{"cob", ",", "cob"}], "]"}], ",", "2", ",", "1"}], "]"}]}], 
     ";", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"coor", "[", 
       RowBox[{"[", 
        RowBox[{"1", ",", "2"}], "]"}], "]"}], "=", "0"}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"cobstyle", "=", 
      RowBox[{"OptionValue", "[", 
       RowBox[{"CobwebPlot", ",", "CobStyle"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"cobstyle", "=", 
      RowBox[{"If", "[", 
       RowBox[{
        RowBox[{"cobstyle", "===", "Automatic"}], ",", "Red", ",", 
        "cobstyle"}], "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"g1", "=", 
      RowBox[{"Graphics", "[", 
       RowBox[{"{", 
        RowBox[{"cobstyle", ",", 
         RowBox[{"Line", "[", "coor", "]"}]}], "}"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"Show", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{
         RowBox[{"Plot", "[", 
          RowBox[{
           RowBox[{"{", 
            RowBox[{"x", ",", 
             RowBox[{"f", "[", "x", "]"}]}], "}"}], ",", 
           RowBox[{"{", 
            RowBox[{"x", ",", "xmin", ",", "xmax"}], "}"}], ",", 
           RowBox[{"PlotStyle", "\[Rule]", 
            RowBox[{"{", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{"Thin", ",", "Black"}], "}"}], ",", 
              RowBox[{"{", 
               RowBox[{"Thick", ",", "Black"}], "}"}]}], "}"}]}]}], "]"}], 
         ",", "g1"}], "}"}], ",", 
       RowBox[{"FilterRules", "[", 
        RowBox[{
         RowBox[{"{", "opts", "}"}], ",", 
         RowBox[{"Options", "[", "Graphics", "]"}]}], "]"}]}], "]"}]}]}], 
   "]"}]}]}], "Input",
 CellChangeTimes->{{3.796412700287119*^9, 3.79641270030486*^9}, {
  3.796475501851125*^9, 3.796475517848262*^9}},
